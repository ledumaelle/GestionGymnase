<AnchorPane id="AnchorPane" prefHeight="259.0" prefWidth="602.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.FXMLAjouterSportController">
   <children>
      <Label id="lblSport" fx:id="lblSport" layoutX="25.0" layoutY="210.0" text="Sport : " />
      <TextField id="txtSport" fx:id="txtSport" layoutX="87.0" layoutY="206.0" prefHeight="25.0" prefWidth="205.0" />
      <Button id="btnAjouter" fx:id="btnAjouter" layoutX="404.0" layoutY="206.0" mnemonicParsing="false" onAction="#handleAjouterSport" text="Ajouter" />
      <Button id="btnAnnuler" fx:id="btnAnnuler" layoutX="479.0" layoutY="206.0" mnemonicParsing="false" onAction="#handleBtnAnnuler" text="Annuler" />
      <ImageView fitHeight="113.0" fitWidth="239.0" layoutX="352.0" layoutY="71.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../ressources/img/AjouterSport.jpg" />
         </image>
      </ImageView>
      <ListView id="lvSport" fx:id="lvSport" layoutX="87.0" layoutY="79.0" prefHeight="101.0" prefWidth="205.0" />
      <Label id="lblListeSport" fx:id="lblListeSport" layoutX="27.0" layoutY="71.0" text="Liste : " />
      <Label layoutX="201.0" layoutY="22.0" text="Afficher un sport " textFill="#4800ff">
